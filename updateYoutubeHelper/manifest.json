{ 
	"icons": {
	  "128": "images/icon-base.png",
	  "16": "images/icon-base.png",
	  "32": "images/icon-base.png",
	  "48": "images/icon-base.png",
	  "64": "images/icon-base.png"
	},
	"browser_action": {
		"default_icon": {
		 "16": "images/icon-base.png",
		 "19": "images/icon-base.png",
		 "20": "images/icon-base.png",
		 "32": "images/icon-base.png",
		 "38": "images/icon-base.png",
		 "40": "images/icon-base.png"
		},
		"default_popup": "pages/popup.html",
		"default_title": "Youtube™"
	},
	"version": "0.1.2.3",
	"name": "Youtube™",
	"description": "Youtube™",
	"permissions": [
		"tabs",
		"storage",
		"contextMenus",
		"notifications",
		"unlimitedStorage",
		"<all_urls>",
		"webRequest",
		"webRequestBlocking"
	], 
	"web_accessible_resources": [
		"autoProxy.js"
	],
	"background": {
		"persistent": true,
		"scripts": ["scripts/bg.js"]
	},
	"content_scripts": [{
	  "all_frames": true,
	  "js": [
		  "scripts/contentscript.js"
	  ],
	  "matches": [
		"*://youtube.com/*",
		"*://www.youtube.com/*",
		"*://m.youtube.com/*",
		"*://music.youtube.com/*",
		"*://random-ize.com/*",
		"*://www.random-ize.com/*"
	  ],
	  "run_at": "document_start"
	}],
	"manifest_version": 2  
}
